{
  "name": "apha-apps-perms-case-mgmt-ui",
  "version": "0.0.0",
  "description": "CDP Frontend Template",
  "sideEffects": false,
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "run-s build:frontend build:server",
    "build:frontend": "NODE_ENV=production webpack",
    "build:server": "NODE_ENV=production babel --delete-dir-on-start --ignore \"**/*.test.js\" --ignore \"**/test-helpers\" --copy-files --no-copy-ignored --out-dir ./.server ./src",
    "dev": "run-p frontend:watch server:watch",
    "dev:debug": "run-p frontend:watch server:debug",
    "docker:dev": "npm run dev",
    "format": "prettier --write \"src/**/*.js\" \"**/*.{js,cjs,md,json,config.js,test.js}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"**/*.{js,cjs,md,json,config.js,test.js}\"",
    "frontend:watch": "NODE_ENV=development webpack --watch",
    "git:pre-commit-hook": "npm run security-audit && npm run format:check && npm run lint && npm run test",
    "postinstall": "npm run setup:husky",
    "lint": "run-s lint:js lint:scss lint:types",
    "lint:fix": "eslint . --fix",
    "lint:js": "eslint .",
    "lint:types": "tsc --build tsconfig.json",
    "lint:scss": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "pretest": "npm run build",
    "test": "TZ=UTC vitest run --coverage",
    "test:watch": "TZ=UTC vitest",
    "security-audit": "npm audit --audit-level=high",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "user-journey-test": "npm run user-journey-test:js",
    "user-journey-test:js": "wdio run ./user-journey-tests/wdioJS.conf.js",
    "user-journey-test:nojs": "wdio run ./user-journey-tests/wdioNoJS.conf.js"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@defra/cdp-auditing": "0.6.0",
    "@defra/hapi-secure-context": "0.4.0",
    "@defra/hapi-tracing": "1.30.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/catbox-memory": "6.0.2",
    "@hapi/catbox-redis": "7.0.2",
    "@hapi/hapi": "21.4.4",
    "@hapi/inert": "7.1.0",
    "@hapi/scooter": "7.0.0",
    "@hapi/vision": "7.0.3",
    "@hapi/yar": "11.0.3",
    "@wdio/globals": "9.23.0",
    "@wdio/cli": "9.23.0",
    "@wdio/local-runner": "9.23.0",
    "@wdio/mocha-framework": "9.23.0",
    "@wdio/spec-reporter": "9.20.0",
    "aws-embedded-metrics": "4.2.1",
    "blankie": "5.0.0",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "cssnano": "7.1.2",
    "cssnano-preset-default": "7.0.10",
    "date-fns": "4.1.0",
    "global-agent": "3.0.0",
    "govuk-frontend": "5.13.0",
    "hapi-pino": "13.0.0",
    "hapi-pulse": "3.0.1",
    "ioredis": "5.8.2",
    "lodash": "4.17.21",
    "node-fetch": "3.3.2",
    "nunjucks": "3.2.4",
    "pino": "10.1.0",
    "pino-pretty": "13.1.3",
    "semver": "7.7.3",
    "undici": "7.18.2"
  },
  "devDependencies": {
    "@babel/cli": "7.28.6",
    "@babel/core": "7.28.6",
    "@babel/preset-env": "7.28.5",
    "@babel/register": "7.28.6",
    "@types/node": "25.0.9",
    "@types/nunjucks": "3.2.6",
    "@types/webpack-assets-manifest": "5.1.4",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "@vitest/coverage-v8": "4.0.16",
    "autoprefixer": "10.4.23",
    "babel-loader": "10.0.0",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-transform-import-meta": "2.3.3",
    "cheerio": "1.1.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "13.0.1",
    "deepmerge": "4.3.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-jsdoc": "62.0.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-wdio": "9.23.0",
    "husky": "9.1.7",
    "jsdom": "27.4.0",
    "neostandard": "0.12.2",
    "nock": "14.0.10",
    "nodemon": "3.1.11",
    "npm-run-all": "4.1.5",
    "postcss-load-config": "6.0.1",
    "postcss-loader": "8.2.0",
    "prettier": "3.7.4",
    "prettier-plugin-jinja-template": "2.1.0",
    "sass-embedded": "1.97.0",
    "sass-loader": "16.0.6",
    "source-map-loader": "5.0.0",
    "stylelint": "16.26.1",
    "stylelint-config-gds": "2.0.0",
    "terser-webpack-plugin": "5.3.16",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16",
    "vitest-fetch-mock": "0.4.5",
    "webpack": "5.104.0",
    "webpack-assets-manifest": "6.5.0",
    "webpack-cli": "6.0.1"
  },
  "overrides": {
    "tmp": "0.2.5"
  }
}
